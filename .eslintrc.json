{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "import/no-extraneous-dependencies" : 0,
    "no-unused-vars" : 0,
    "no-underscore-dangle" : 0,
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}